using System;
using System.Collections.Generic;
using System.IO;

class Solution {
    static void Main(String[] args) {
        int i = 4;
        double d = 4.0;
        string s = "HackerRank ";

        
        // Declare second integer, double, and String variables.
        int intergerVariable;
        double doubleVariable;
        string stringVariable;
        // Read and save an integer, double, and String to your variables.
        intergerVariable = int.Parse(Console.ReadLine());
        doubleVariable = double.Parse(Console.ReadLine());
        stringVariable = Console.ReadLine();
        // Print the sum of both integer variables on a new line.
        Console.WriteLine(i+intergerVariable);
        // Print the sum of the double variables on a new line.
        Console.WriteLine( string.Format("{0:0.0}", d+doubleVariable));
        // Concatenate and print the String variables on a new line
        // The 's' variable above should be printed first.
        Console.WriteLine(s+stringVariable);
    }
